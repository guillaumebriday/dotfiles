{
  "before action": {
    "prefix": "ba",
    "body": [
      "before_action :$1"
    ],
    "description": "Print before_action."
  },

  "method index": {
    "prefix": "index",
    "body": [
      "def index",
      "\t@${1:resources} = ${2:model}.apply_filters(params).paginate(per_page: 20, page: params[:page])",
      "end\n"
    ],
    "description": "Print method index."
  },

  "byebug": {
    "prefix": "b",
    "body": [
      "byebug"
    ],
    "description": "Print byebug."
  },

  "frozen_string_literal": {
    "prefix": "fsl",
    "body": [
      "# frozen_string_literal: true",
      "\n"
    ],
    "description": "Print frozen_string_literal magic comment"
  },

  "strong params": {
    "prefix": "sp",
    "body": [
      "def $1_params",
      "\tparams",
      "\t\t.require(:$1)",
      "\t\t.permit(",
      "\t\t\t:$2",
      "\t\t)",
      "end"
    ],
    "description": "Print strong params method"
  },

  "add_column": {
    "prefix": "add_column",
    "body": "add_column :${1:table}, :${2:column}, :${3:string}",
    "description": "add_column :table, :column, :type",
  }
}
